{
    "compilerOptions": {
      "target": "ES6",
      "module": "ES6",
      "moduleResolution": "node",
      "lib": ["ES6", "DOM"],
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "noImplicitAny": true,
      "outDir": "lib"
    },
    "include": ["src/**/*"]
  }
  